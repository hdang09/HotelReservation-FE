{
  "printWidth": 120,
  "tabWidth": 2,
  "bracketSpacing": true,
  "trailingComma": "all",
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "singleQuote": true,
  "semi": false,
  "endOfLine": "auto",
  "importOrder": [
    "^react$",
    "^[a-z\\-/]*$",
    "(^@material-ui|components|providers)",
    "(^[src/]*$|^[./]|^[../])",
    ""
  ],
  "importOrderSeparation": false
}
